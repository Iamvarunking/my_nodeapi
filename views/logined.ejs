<table border="1" cellpadding='7' cellspacing='7'>
    <tr>
        <th>No</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Father's Name</th>
        <th>Branch</th>
        <th>Username</th>
        <th>Password</th>
        <th>Image</th>
        <th>Delete</th>
        <th>Edit</th>
        <th>Log Out</th>
    </tr>
    <% if(data.length){
        for(var i=0; i<data.length ; i++)
    
    {
        %>
    <tr>
        <td><%= data[i].id %></td>
        <td><%= data[i].first_name %></td>
        <td><%= data[i].last_name %></td>
        <td><%= data[i].father_name %></td>
        <td><%= data[i].branch %></td>
        <td><%= data[i].username %></td>
        <td><%= data[i].password %></td>
        <td><img src="/../uploads/<%= data[i].image %>" width="100px" height="100px"></td>
        <td><a href="/delete/<%= data[i].id %>">Delete</a></td>
        <td><a href="/edit/<%= data[i].id %>">Edit</a></td>
        <td><a href="/logout">Log out</a></td>
    </tr>
    <% }
}
else{ %> 
    <tr>
        <td colspan="3">No user fond</td>
    </tr>
    <% } %>
</table>